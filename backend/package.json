{
  "name": "quicksell-backend",
  "version": "1.0.0",
  "description": "QuickSell - Photo to Marketplace Selling App Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "selling",
    "marketplace",
    "ebay",
    "facebook",
    "craigslist"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.0.0",
    "dotenv": "^16.0.3",
    "pg": "^8.9.0",
    "redis": "^4.6.5",
    "jwt-simple": "^0.5.6",
    "bcryptjs": "^2.4.3",
    "axios": "^1.3.5",
    "sharp": "^0.32.0",
    "multer": "^1.4.5-lts.1",
    "bull": "^4.10.4",
    "openai": "^4.68.0",
    "stripe": "^11.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.7.0",
    "joi": "^17.9.1",
    "morgan": "^1.10.0",
    "winston": "^3.8.2",
    "knex": "^2.5.1",
    "node-cache": "^5.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@types/sharp": "^0.32.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.0"
  }
}
